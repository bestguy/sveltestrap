{
  "name": "sveltestrap",
  "version": "5.11.2",
  "main": "dist/sveltestrap.js",
  "module": "dist/sveltestrap.es.js",
  "jsnext:main": "dist/sveltestrap.es.js",
  "svelte": "src/index.js",
  "types": "src/index.d.ts",
  "type": "module",
  "keywords": [
    "svelte"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:bestguy/sveltestrap.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "start": "storybook dev -p 6006",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "dist": "rollup -c",
    "docs": "storybook build -o docs && cp CNAME ./docs && cp -r ./v4 ./docs",
    "format": "prettier --write --plugin-search-dir=. ./src ./stories ./.storybook",
    "lint": "tsc --noEmit && eslint ./src --ext .js --ext .svelte",
    "prepublish": "npm run dist && git add -A dist",
    "postpublish": "git push && git push --tags",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --maxWorkers=2 --verbose --coverage",
    "test:watch": "jest --maxWorkers=2 --verbose --watchAll",
    "version": "npm run dist && npm run docs && git add -A"
  },
  "peerDependencies": {
    "svelte": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@storybook/addon-actions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/addon-storysource": "^7.4.6",
    "@storybook/addons": "^7.4.6",
    "@storybook/svelte": "^7.4.6",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/svelte": "^4.0.4",
    "@tsconfig/svelte": "^5.0.2",
    "@types/jest": "^29.5.5",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "conventional-changelog-cli": "^4.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-interactions": "^7.2.1",
    "@storybook/blocks": "^7.2.1",
    "@storybook/svelte-webpack5": "^7.2.1",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/theming": "^7.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-svelte": "^2.34.0",
    "get-port-cli": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prismjs": "^1.29.0",
    "raw-loader": "^4.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-svelte": "^7.1.6",
    "sirv-cli": "^2.0.2",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.2",
    "svelte-jester": "^3.0.0",
    "svelte-loader": "^3.1.9",
    "svelte-preprocess": "^5.0.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "wait-for-localhost-cli": "^3.0.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "storybook": "^7.2.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8"
  },
  "browserslist": ["last 2 versions"]
}
